
#ino clean

ino build -m leonardo
#ino build -m mega2560
#ino build -m uno

until ino upload -m leonardo
  do 
    echo "didn't upload"
  done

sleep 1
#This replicates "newtab" function because of some weird environment thing.
osascript -e 'tell application "Terminal" to activate' \
  -e 'tell application "System Events" to tell process "Terminal" to keystroke "t" using command down' \
  -e 'tell application "Terminal" to do script "screen /dev/tty.usbmodem1421 57600" in selected tab of the front window'
